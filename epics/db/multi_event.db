record (calcout, "${UUT}:${SITE}:MEV:c")
{
        field(DTYP, "stream")
        field(INPA, "${UUT}:${SITE}:MEV:PRE")
        field(INPB, "${UUT}:${SITE}:MEV:POST")
	field(INPC, "${UUT}:${SITE}:MEV:MAX")
        field(INPD, "${UUT}:${SITE}:MEV:c")
        field(CALC, "VAL=D+1")
	field(OUT,  "@multi_event.proto setMEV ${SPORT}")
        field(OOPT, "On Change")
        field(DOPT, "Use OCAL")
        field(OCAL, "-999")
}

